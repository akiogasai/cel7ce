(= step (fn ()))

(= keydown (fn (k)
    (color 0x01)
    (fill 0 0 width height " ")
    (put 1 1 "keydown " k)
))

(= mouse (fn (type evnum x y)
    (color 0x01)
    (fill 0 0 width height " ")
    (put 1 1 type evnum)
    (put 1 2 "x: " x)
    (put 1 3 "y: " y)
    (color 0x10)
    (put x y " ")
))
